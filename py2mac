SCRIPT_PATH="${BASH_SOURCE[0]}";
if ([ -h "${SCRIPT_PATH}" ]) then
  while([ -h "${SCRIPT_PATH}" ]) do SCRIPT_PATH=`readlink "${SCRIPT_PATH}"`; done
fi
pushd . > /dev/null
cd `dirname ${SCRIPT_PATH}` > /dev/null
SCRIPT_PATH=`pwd`;

arch=`uname -m`
out=appbin_daemon_mac
app=.app
rm -rf dist/$out
rm -rf dist/$out$app
rm -rf build
VERSIONER_PYTHON_PREFER_32_BIT=yes arch -i386 python `which pyinstaller` --clean -n $out src/appbin_daemon.py
